                            <div class="row g-2">
                            <div class="col mb-0">
                              <button
                              type="button"
                              class="btn btn-primary"
                              data-bs-toggle="modal"
                              data-bs-target="#modalCenter">
                              Upload
                            </button>
                            </div>
                            <% if (dataUser.rencanaKerja == undefined) { %>
                            <div style="display: none;" class="col mb-0">
                              <a href="<%= dataUser.rencanaKerja %>" target="_blank">
                                <button type="button" class="btn btn-secondary">Lihat File</button>
                              </a>
                            </div>
                            <% } else { %>
                              <div class="col mb-0">
                                <a href="<%= dataUser.rencanaKerja %>" target="_blank">
                                  <button type="button" class="btn btn-secondary">Lihat File</button>
                                </a>
                              </div>
                              <% } %> 
                            </div>
                           
                          
                        <!-- Modal -->
                        <form action="/upload/<%=dataUser.id %>" method="post" enctype="multipart/form-data">
                        <div class="modal fade" id="modalCenter" tabindex="-1" aria-hidden="true">
                          <div class="modal-dialog modal-dialog-centered" role="document">
                            <div class="modal-content">
                              <div class="modal-header">
                                <h5 class="modal-title" id="modalCenterTitle">Berkas Rencana Kerja</h5>
                                <button
                                  type="button"
                                  class="btn-close"
                                  data-bs-dismiss="modal"
                                  aria-label="Close"></button>
                              </div>
                              <div class="modal-body">
                                <div class="row">
                                  <div class="col mb-3">
                                    <label for="formFile" class="form-label">Upload Berkas</label>
                                    <input required class="form-control" name="pdf" type="file" id="formFile" />
                                  </div>
                                </div>
                              </div>
                              <div class="modal-footer">
                                <button type="submit" class="btn btn-primary">Upload</button>
                              </div>
                            </div>
                          </div>
                        </div>
                      </form>
