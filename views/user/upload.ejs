<%- include("../layouts/headerUser") %>

<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<% if (status == "success") { %>
    <script>
      Swal.fire({
        icon: 'success',
        title: "<%= message %>",
        timer: 1500,
      })
   </script>
<% } else if (status == "failed") { %>
    <script>
      Swal.fire({
        icon: 'error',
        title: "<%= message %>",
        timer: 1500,
      })
   </script>
  <% } %> 
